<div class="container my-5">


  <h1><%= @list.name %></h1>
  <% @list.movies.each do |movie| %>
  <%= movie.title %>
  <% end %>

<%= link_to "Add a movie", new_list_bookmark_path(@list) %>
<%= link_to 'Delete this list', destroy_user_session_path,
  data: {turbo_method: :delete, turbo_confirm: 'Are you sure?'} %>

</div>
